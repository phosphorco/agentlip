{
  "name": "@agentlip/client",
  "version": "0.1.1-rc.1",
  "description": "TypeScript SDK for connecting to Agentlip hubs",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/phosphorco/agentlip",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phosphorco/agentlip.git",
    "directory": "packages/client"
  },
  "engines": {
    "bun": ">=1.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/**/*",
    "src/**/*.ts",
    "!src/**/*.test.ts"
  ],
  "exports": {
    ".": {
      "bun": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./local": {
      "bun": "./src/local.ts",
      "types": "./dist/local.d.ts",
      "import": "./dist/local.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json"
  },
  "dependencies": {
    "@agentlip/workspace": "^0.1.0",
    "@agentlip/protocol": "^0.1.0"
  }
}
